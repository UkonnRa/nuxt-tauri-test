{
  "name": "white-rabbit",
  "productName": "White Rabbit",
  "description": "Alice's Wonderland White Rabbit. Working for family / personal accounting.",
  "author": "Ukonn Ra <ukonnra@outlook.com>",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.1",
  "engines": {
    "yarn": ">=4",
    "node": ">=22"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "husky && yarn build:buf",
    "upgrade": "yarn upgrade-interactive && concurrently \"rimraf '.yarn/cache' 'node_modules/' 'yarn.lock' && yarn\" \"cargo update && cargo upgrade --incompatible --pinned\"",
    "dev:tauri": "yarn workspace @white-rabbit/app-tauri run tauri dev",
    "dev:ssr": "yarn workspace @white-rabbit/app-ssr run dev",
    "lint:rs": "cargo fmt --check && cargo clippy",
    "lint:fix:rs": "cargo sort -w && cargo fmt && cargo clippy --fix --allow-dirty --allow-staged",
    "lint:stylelint": "stylelint '**/*.{vue,scss,css}' --ignore-path .prettierignore",
    "lint:prettier": "prettier '**/*.{vue,ts,js,json}'",
    "lint:eslint": "yarn workspaces foreach --from 'packages/*' -Rpitv run lint:eslint",
    "lint:tsc": "yarn workspaces foreach --from 'packages/*' -Rpitv run lint:tsc",
    "lint:fix": "concurrently \"yarn lint:prettier --write && yarn lint:eslint --fix\" \"yarn lint:stylelint --fix\" \"yarn lint:fix:rs\"",
    "lint": "concurrently \"buf lint\" \"yarn lint:prettier --check\" \"yarn lint:stylelint\" \"yarn lint:rs\" \"yarn lint:eslint\" \"yarn lint:tsc\"",
    "build": "concurrently \"yarn build:buf && yarn workspaces foreach --from 'packages/*' --exclude 'packages/app-tauri' -Rpitv run build\" \"yarn build:rs\"",
    "build:rs": "cargo build --workspace --exclude app-tauri",
    "build:buf": "rimraf packages/app-ssr/server/proto/gen; buf generate",
    "build:tauri": "yarn workspace @white-rabbit/app-tauri run build",
    "test": "cargo test --workspace && yarn test:vitest",
    "test:vitest": "yarn workspaces foreach --from 'packages/*' -Rpitv run test",
    "test:coverage": "yarn test:coverage:rs && yarn test:vitest --coverage",
    "test:coverage:rs": "rimraf target/debug/coverage; yarn test:coverage:test && yarn test:coverage:generate -t html -o target/debug/coverage/ && yarn test:coverage:generate -t lcov -o target/debug/coverage/lcov.info",
    "test:coverage:test": "CARGO_INCREMENTAL='0' RUSTDOCFLAGS='-Cpanic=abort' RUSTFLAGS='-Zprofile -Ccodegen-units=1 -Copt-level=0 -Clink-dead-code -Coverflow-checks=off -Zpanic_abort_tests -Cpanic=abort' cargo +nightly test --workspace",
    "test:coverage:generate": "grcov . -s . --binary-path ./target/debug/ --branch --keep-only '{packages,crates}/*'"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.45.0",
    "@bufbuild/protobuf": "^2.2.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@nuxt/eslint": "^0.6.0",
    "@nuxt/test-utils": "^3.14.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.3",
    "@vue/compiler-dom": "^3.5.12",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.29.1",
    "globals": "^15.11.0",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.4",
    "sass-loader": "^16.0.2",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.3",
    "vue": "^3.5.12",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
