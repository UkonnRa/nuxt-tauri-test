{
  "productName": "UkonnRa WhiteRabbit",
  "version": "0.1.0",
  "identifier": "com.ukonnra.whiterabbit",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn generate",
    "frontendDist": "../.output/public/"
  },
  "app": {
    "windows": [
      {
        "title": "UkonnRa WhiteRabbit",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}